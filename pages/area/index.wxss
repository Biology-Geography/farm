.A-page .a-title {
  width: 100%;
  height: 30px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #2e8b57;
  box-shadow: 0 5rpx 10rpx 0 #045533;
}
.A-page .a-title text {
  color: white;
  font-size: 40rpx;
}
.A-page .a-title image {
  width: 70rpx;
  height: 70rpx;
}
.A-page .a-Noplan {
  color: white;
  font-size: 40rpx;
}
.A-page .a-item {
  border-bottom: 1px solid skyblue;
  display: flex;
  align-items: center;
  height: 40px;
}
.A-page .a-item text {
  color: #c0bdbd;
  font-size: 40rpx;
}
.A-page .a-Noplace {
  height: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #c0bdbd;
}
