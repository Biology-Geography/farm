page {
  background-color: #2e8b57;
}
.Qr-page {
  background-color: white;
}
.Qr-page .Qr-info {
  margin-top: 25%;
  display: flex;
}
.Qr-page .Qr-info image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.Qr-page .Qr-info label {
  padding-left: 20px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 40rpx;
}
.Qr-page .Qr-code .Qr-Image {
  display: flex;
  justify-content: center;
  align-items: center;
}
.Qr-page .Qr-code .Qr-Image image {
  width: 500rpx;
  height: 500rpx;
}
.Qr-page .Qr-code label {
  display: flex;
  justify-content: center;
  align-items: center;
  color: grey;
}
