.No_login {
  display: flex;
  flex-direction: column;
  background-color: #f5f5f5;
  border: 2rpx solid #a9a9a9;
  box-shadow: 8rpx 4rpx 4rpx #707b8b;
  border-radius: 15rpx;
}
.No_login .img_login {
  width: 150rpx;
  height: 150rpx;
}
.No_login label {
  color: #a9a9a9;
}
.user_img_wrap .user_info_group {
  width: 100%;
  height: 150rpx;
  position: relative;
  display: flex;
  background-color: #2e8b60;
}
.user_img_wrap .user_info_group .user_icon {
  position: absolute;
  width: 150rpx;
  height: 150rpx;
  margin-left: 5%;
  border-radius: 50%;
}
.user_img_wrap .user_info_group .user_name {
  padding-left: 30%;
  padding-top: 5%;
  color: white;
}
.user_img_wrap .user_info_group cover-image {
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  margin-top: 5%;
  margin-left: 80%;
}
