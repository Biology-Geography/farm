page {
  width: 100%;
  height: 100%;
  background-color: #ebebe6;
}
.user_info_wrap .user_img_wrap {
  height: 200rpx;
  background-color: #fdf6f6;
  box-shadow: 3px 3px 4px #bdb9b9;
  display: flex;
  justify-content: space-between;
}
.user_info_wrap .user_img_wrap .user_info_group {
  display: flex;
  align-items: center;
}
.user_info_wrap .user_img_wrap .user_info_group image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 25rpx;
}
.user_info_wrap .user_img_wrap .user_info_group text {
  font-size: 40rpx;
  font-family: '微软雅黑';
  font-weight: border;
  color: balck;
}
.user_info_wrap .user_img_wrap .user_erweima {
  margin-top: 45px;
  margin-right: 30px;
  display: flex;
  align-items: center;
}
.user_info_wrap .user_img_wrap .user_erweima .img1 {
  width: 45rpx;
  height: 45rpx;
  padding-right: 5px;
}
.user_info_wrap .user_img_wrap .user_erweima .img2 {
  width: 30rpx;
  height: 30rpx;
}
.user_info_wrap .icon {
  margin-top: 20px;
  background-color: #fdf6f6;
  height: 200rpx;
}
.user_info_wrap .icon .user_info_symbol {
  display: flex;
  justify-content: space-around;
}
.user_info_wrap .icon .user_info_symbol cover-image {
  margin-top: 50rpx;
  width: 80rpx;
  height: 80rpx;
}
.user_info_wrap .icon .user_info_biaoyu {
  margin-top: 10px;
  display: flex;
  justify-content: space-around;
  font-size: 35rpx;
  color: #0f0114;
}
.user_info_wrap .No_login ._login {
  height: 200rpx;
  background-color: #fdf6f6;
  box-shadow: 3px 3px 4px #bdb9b9;
  display: flex;
  align-items: center;
}
.user_info_wrap .No_login ._login image {
  width: 150rpx;
  height: 150rpx;
}
.user_info_wrap .No_login ._login label {
  font-size: 40rpx;
  font-family: "微软雅黑";
  padding-left: 20rpx;
  color: #aca5a5;
}
