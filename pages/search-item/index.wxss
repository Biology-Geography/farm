.s-nav {
  border: 1px solid #cfd0e0;
  box-shadow: 3px 3px 4px #bdb9b9;
}
.s-nav .nav {
  margin-top: 10px;
  display: flex;
  justify-content: space-between;
  background-color: white;
}
.s-nav .nav .nav1 {
  display: flex;
  border-radius: 50rpx;
  border: 1px solid #ebebeb;
  width: 80%;
  height: 70rpx;
  background-color: #ebebeb;
  align-items: center;
}
.s-nav .nav .nav1 image {
  width: 50rpx;
  height: 50rpx;
}
.s-nav .nav .nav1 input {
  color: grey;
}
.s-nav .nav .nav-T {
  margin-right: 5px;
}
.s-nav .nav .nav-T image {
  width: 70rpx;
  height: 70rpx;
}
.s-nav .tip {
  font-size: 35rpx;
  color: #c0bebe;
}
.s-Plantation {
  margin-top: 10px;
}
.s-Plantation .s-items .s-item {
  height: 230rpx;
}
.s-Plantation .s-items .s-item .s-content {
  display: flex;
  justify-content: space-evenly;
}
.s-Plantation .s-items .s-item .s-content image {
  width: 200rpx;
  height: 200rpx;
  box-shadow: 3px 3px 2px #ebebeb;
}
.s-Plantation .s-items .s-item .s-content .s-remark {
  width: 50%;
  border-bottom: 1px solid #724ccc;
  display: flex;
  flex-direction: column;
  line-height: 30px;
}
